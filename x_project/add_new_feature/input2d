%
% FLUID PARAMETER VALUES %
%
Fluid_Parameters {
mu = 0.001              % Dynamic Viscosity (N*s/m^2) 
rho = 1                 % Density (kg/m^2)
}

%
% TEMPORAL INFORMATION %
%
Temporal_Information {
Tfinal = 1.0/0.2        % Final time of simulation (s)
dt =  1.0e-5/5        % Time-step (s)
}

%
% GRID PARAMETERS %
%
Grid_Parameters {
Nx = 512                % # of Eulerian Grid Pts. in x-Direction (MUST BE EVEN!!!)
Ny = 128                % # of Eulerian Grid Pts. in y-Direction (MUST BE EVEN!!!)
Lx = 2.0                % Length of Eulerian Grid in x-Direction (m)
Ly = 0.5                % Length of Eulerian Grid in y-Direction (m)
supp = 4                % Choose dirac-delta support (KEEP IT EVEN!!!)
}

%
% LAGRANGIAN STRUCTURE MODEL INFORMATION %
%
Lag_Structure_Info {
springs = 0             % 0 (for no) or 1 (for yes)
